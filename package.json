{
  "name": "superpipe",
  "version": "0.14.0",
  "description": "The missing pipeline for functional programming",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es/**/*",
    "lib/**/*",
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=development mocha --recursive --require source-map-support --require ./test/babel-register",
    "watch": "nodemon -w ./src -w ./test -x 'npm run test'",
    "coverage": "nyc npm test",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:es": "cross-env NODE_ENV=production tsc -d --noEmit false --module es6 --outDir ./es",
    "build:cjs": "cross-env NODE_ENV=production babel -x '.ts' -d ./lib src",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/superpipe.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/superpipe.min.js",
    "build:types": "tsc -d --emitDeclarationOnly --noEmit false --outDir ./lib",
    "build": "npm run build:es && npm run build:cjs && npm run build:types && npm run build:umd && npm run build:umd:min"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.4.4",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint_d": "7.3.0",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "rollup": "1.15.6",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.3",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-uglify": "6.0.2",
    "typescript": "3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lsm/superpipe.git"
  },
  "keywords": [
    "flow",
    "stream",
    "pipe",
    "pipeline",
    "event",
    "ioc",
    "frp",
    "functional",
    "reactive",
    "monad"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "lsm <marc@liu.pm> (https://github.com/lsm)",
  "license": "MIT",
  "browserslist": {
    "development": [
      "last 1 chrome versions",
      "last 1 firefox versions",
      "last 1 safari version"
    ],
    "production": [
      "> 0.25%",
      "not dead"
    ]
  }
}
